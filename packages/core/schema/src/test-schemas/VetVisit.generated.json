{
  "$defs": {
    "Pet": {
      "title": "Pet",
      "description": "My pets.\n\n## Enum members\n\n### Galois\n\nCat.\n\n### Abel\n\nDog.",
      "type": "string",
      "enum": [
        "Galois",
        "Abel"
      ]
    },
    "JsonObject": {
      "title": "JsonObject",
      "type": "object",
      "properties": {
        "__dataType": {
          "type": "string",
          "const": "JsonObject"
        }
      },
      "required": [
        "__dataType"
      ],
      "additionalProperties": true
    }
  },
  "title": "VetVisit",
  "description": "A visit to the vet.",
  "type": "object",
  "properties": {
    "pet": {
      "description": "Which pet was brought.",
      "$ref": "#/$defs/Pet"
    },
    "date": {
      "description": "Date of the visit.\n\n## Format `dev.superego:String.PlainDate`\n\nA calendar date in the ISO8601 format, not associated with a particular time or time zone.",
      "examples": [
        "2006-08-24",
        "2024-02-29"
      ],
      "type": "string"
    },
    "title": {
      "description": "Short title for the visit. 5 words max.",
      "type": "string"
    },
    "vet": {
      "description": "Which vet the pet was brought to.",
      "type": "string"
    },
    "notes": {
      "description": "Details about the visit. What the vet said, what they prescribed, etc.\n\n## Format `dev.superego:JsonObject.TiptapRichText`\n\nA rich-text document as represented, in JSON, by the Tiptap rich-text editor.",
      "examples": [
        {
          "__dataType": "JsonObject",
          "type": "doc",
          "content": []
        },
        {
          "__dataType": "JsonObject",
          "type": "doc",
          "content": [
            {
              "type": "paragraph",
              "attrs": {
                "textAlign": null
              },
              "content": [
                {
                  "type": "text",
                  "text": "Hello, World!"
                }
              ]
            }
          ]
        }
      ],
      "anyOf": [
        {
          "$ref": "#/$defs/JsonObject"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "pet",
    "date",
    "title",
    "vet",
    "notes"
  ]
}