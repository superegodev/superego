{
  "$defs": {
    "MassQuantity": {
      "title": "MassQuantity",
      "description": "A quantity of mass.",
      "type": "object",
      "properties": {
        "unit": {
          "description": "Grams.",
          "type": "string",
          "const": "g"
        },
        "amount": {
          "type": "number"
        }
      },
      "additionalProperties": false,
      "required": [
        "unit",
        "amount"
      ]
    }
  },
  "title": "FoodJournalEntry",
  "type": "object",
  "properties": {
    "date": {
      "description": "Format `dev.superego:String.PlainDate`:\n\nA calendar date in the ISO8601 format, not associated with a particular time or time zone.",
      "examples": [
        "2006-08-24",
        "2024-02-29",
        "-000924-01-01",
        "0924-01-01",
        "+010924-01-01"
      ],
      "type": "string"
    },
    "meals": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "time": {
            "description": "Format `dev.superego:String.Instant`:\n\nAn exact point in time in the ISO8601 format, in \"Zulu time\", with millisecond precision.",
            "examples": [
              "2006-08-24T19:39:09.000Z",
              "2006-08-24T19:39:09.068Z"
            ],
            "type": "string"
          },
          "consumedFoods": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "foodId": {
                  "type": "string"
                },
                "quantity": {
                  "$ref": "#/$defs/MassQuantity"
                }
              },
              "additionalProperties": false,
              "required": [
                "foodId",
                "quantity"
              ]
            }
          }
        },
        "additionalProperties": false,
        "required": [
          "time",
          "consumedFoods"
        ]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "date",
    "meals"
  ]
}