{
  "$defs": {
    "FuelType": {
      "title": "FuelType",
      "description": "Type of fuel that can be used for refuelling.\n\n## Enum members\n\n### Gasoline 95\n\n95 octane gasoline.\n\n### Gasoline 98\n\n98 octane gasoline.",
      "type": "string",
      "enum": [
        "Gasoline 95",
        "Gasoline 98"
      ]
    },
    "Vehicle": {
      "title": "Vehicle",
      "description": "My vehicles.\n\n## Enum members\n\n### Kia Sportage\n\nMy main car.\n\n### Kawasaki Ninja\n\nMy motorbike.",
      "type": "string",
      "enum": [
        "Kia Sportage",
        "Kawasaki Ninja"
      ]
    },
    "JsonObject": {
      "title": "JsonObject",
      "type": "object",
      "properties": {
        "__dataType": {
          "type": "string",
          "const": "JsonObject"
        }
      },
      "required": [
        "__dataType"
      ],
      "additionalProperties": true
    }
  },
  "title": "FuelLogEntry",
  "description": "A single refuelling event.",
  "type": "object",
  "properties": {
    "vehicle": {
      "description": "Which vehicle was refuelled.",
      "$ref": "#/$defs/Vehicle"
    },
    "timestamp": {
      "description": "Timestamp of the refueling event.\n\n## Format `dev.superego:String.Instant`\n\nAn exact point in time in the ISO8601 format, with a specified offset, with millisecond precision.",
      "examples": [
        "2006-08-24T19:39:09.000Z",
        "2006-08-24T19:39:09.068Z"
      ],
      "type": "string"
    },
    "odometer": {
      "description": "Odometer reading at the time of refueling, in kilometers.",
      "type": "number"
    },
    "liters": {
      "description": "Number of liters of fuel added.",
      "type": "number"
    },
    "totalCost": {
      "description": "Total cost of refueling.",
      "type": "number"
    },
    "fullTank": {
      "description": "Indicates if the tank was filled completely.",
      "type": "boolean"
    },
    "pricePerLiter": {
      "description": "Price per liter of fuel.",
      "type": [
        "number",
        "null"
      ]
    },
    "fuelType": {
      "description": "Type of fuel used for the refuelling.",
      "anyOf": [
        {
          "$ref": "#/$defs/FuelType"
        },
        {
          "type": "null"
        }
      ]
    },
    "notes": {
      "description": "Any additional notes.\n\n## Format `dev.superego:JsonObject.TiptapRichText`\n\nA rich-text document as represented, in JSON, by the Tiptap rich-text editor.",
      "examples": [
        {
          "__dataType": "JsonObject",
          "type": "doc",
          "content": []
        },
        {
          "__dataType": "JsonObject",
          "type": "doc",
          "content": [
            {
              "type": "paragraph",
              "attrs": {
                "textAlign": null
              },
              "content": [
                {
                  "type": "text",
                  "text": "Hello, World!"
                }
              ]
            }
          ]
        }
      ],
      "anyOf": [
        {
          "$ref": "#/$defs/JsonObject"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "vehicle",
    "timestamp",
    "odometer",
    "liters",
    "totalCost",
    "fullTank",
    "pricePerLiter",
    "fuelType",
    "notes"
  ]
}