{
  "name": "@superego/electron-app",
  "productName": "superego",
  "description": "Your Digital Freedom",
  "version": "2026.2.2",
  "type": "module",
  "private": true,
  "main": "./dist/main/index.js",
  "author": {
    "name": "Paolo Scanferla",
    "email": "paolo.scanferla@gmail.com",
    "url": "https://pscanf.com/"
  },
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "clear-vite-cache": "rm -rf node_modules/.vite node_modules/.vite-temp",
    "check-types": "tsc --noEmit",
    "check-translations": "node scripts/checkTranslations.ts",
    "translations:extract": "formatjs extract 'src/main/**/*.ts' --ignore='**/*.d.ts' --id-interpolation-pattern='[name].[ext]_[sha512:contenthash:base64:6]' --out-file src/main/translations/en.json",
    "translations:compile": "formatjs compile-folder --ast src/main/translations/ src/main/translations/compiled",
    "translations:verify": "formatjs verify --missing-keys --structural-equality --source-locale en 'src/main/translations/*.json'",
    "translations:extract-and-compile": "yarn translations:extract && yarn translations:compile",
    "build:cli": "vite --config=cli.vite.config.ts build",
    "build:devenv": "vite --config=devenv.vite.config.ts build",
    "build:electron-vite": "electron-vite --outDir=dist build",
    "build:forge": "electron-forge publish --dry-run",
    "build": "yarn build:cli && yarn build:electron-vite && yarn build:devenv && yarn build:forge",
    "deploy": "electron-forge publish --from-dry-run"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron/fuses": "^2.0.0",
    "@formatjs/cli": "^6.13.0",
    "@reforged/maker-appimage": "^5.2.0",
    "@rollup/plugin-babel": "^6.1.0",
    "babel-plugin-formatjs": "^11.2.4",
    "electron": "40.6.0",
    "electron-vite": "^5.0.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@formatjs/intl": "^4.1.2",
    "@superego/app-sandbox": "workspace:*",
    "@superego/backend": "workspace:*",
    "@superego/browser-app": "workspace:*",
    "@superego/cli": "workspace:*",
    "@superego/connectors": "workspace:*",
    "@superego/demo-data-repositories": "workspace:*",
    "@superego/executing-backend": "workspace:*",
    "@superego/global-types": "workspace:*",
    "@superego/openai-compat-inference-service": "workspace:*",
    "@superego/quickjs-javascript-sandbox": "workspace:*",
    "@superego/schema": "workspace:*",
    "@superego/shared-utils": "workspace:*",
    "@superego/sqlite-data-repositories": "workspace:*",
    "@superego/tsc-typescript-compiler": "workspace:*",
    "filenamify": "^7.0.1"
  }
}
