{
  "name": "@superego/electron-app",
  "productName": "superego",
  "description": "Your Life's Database",
  "version": "2025.9.3",
  "main": "./dist/main/index.mjs",
  "scripts": {
    "dev": "NODE_OPTIONS=\"--disable-warning=ExperimentalWarning\" electron-vite --outDir=dist dev",
    "clear-vite-cache": "rm -rf node_modules/.vite node_modules/.vite-temp",
    "check-types": "tsc --noEmit",
    "build:vite": "electron-vite --outDir=dist build",
    "build:forge": "electron-forge publish --dry-run",
    "build": "yarn build:vite && yarn build:forge",
    "deploy": "electron-forge publish --from-dry-run"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/publisher-github": "^7.9.0",
    "@electron/fuses": "^2.0.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "electron": "38.2.0",
    "electron-vite": "^4.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@superego/backend": "workspace:*",
    "@superego/browser-app": "workspace:*",
    "@superego/executing-backend": "workspace:*",
    "@superego/global-types": "workspace:*",
    "@superego/openai-compat-inference-service": "workspace:^",
    "@superego/quickjs-javascript-sandbox": "workspace:*",
    "@superego/sqlite-data-repositories": "workspace:*",
    "electron-squirrel-startup": "^1.0.1"
  }
}
