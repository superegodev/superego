{
  "name": "@superego/browser-app",
  "version": "2025.9.3",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.tsx"
    },
    "./vite.config.js": {
      "import": "./vite.config.ts"
    }
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "clear-vite-cache": "rm -rf node_modules/.vite node_modules/.vite-temp",
    "check-types": "tsc --noEmit",
    "test": "vitest --reporter=verbose",
    "check-translations": "node scripts/checkTranslations.ts",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --id-interpolation-pattern='[name].[ext]_[sha512:contenthash:base64:6]' --out-file src/translations/en.json",
    "translations:compile": "formatjs compile-folder --ast src/translations/ src/translations/compiled",
    "translations:verify": "formatjs verify --missing-keys --structural-equality --source-locale en 'src/translations/*.json'",
    "translations:extract-and-compile": "yarn translations:extract && yarn translations:compile"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@formatjs/cli": "^6.7.3",
    "@superego/connectors": "workspace:*",
    "@superego/demo-data-repositories": "workspace:^",
    "@superego/executing-backend": "workspace:*",
    "@superego/openai-compat-inference-service": "workspace:^",
    "@superego/quickjs-javascript-sandbox": "workspace:*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/luxon": "^3.7.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.56.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4",
    "zx": "8.8.4-lite"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-aria/optimize-locales-plugin": "^1.1.5",
    "@standard-schema/spec": "^1.0.0",
    "@superego/backend": "workspace:*",
    "@superego/global-types": "workspace:*",
    "@superego/javascript-sandbox-global-utils": "workspace:*",
    "@superego/schema": "workspace:*",
    "@superego/shared-utils": "workspace:^",
    "@tanstack/react-query": "^5.90.2",
    "@tiptap/extension-code-block-lowlight": "3.6.5",
    "@tiptap/extension-list": "3.6.5",
    "@tiptap/extension-subscript": "3.6.5",
    "@tiptap/extension-superscript": "3.6.5",
    "@tiptap/extension-text-align": "3.6.5",
    "@tiptap/extension-text-style": "3.6.5",
    "@tiptap/extension-typography": "3.6.5",
    "@tiptap/extensions": "3.6.5",
    "@tiptap/pm": "3.6.5",
    "@tiptap/react": "3.6.5",
    "@tiptap/starter-kit": "3.6.5",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-formatjs": "^10.5.40",
    "debounce": "^2.2.0",
    "echarts": "^6.0.0",
    "es-toolkit": "^1.40.0",
    "frimousse": "^0.3.0",
    "lowlight": "^3.3.0",
    "markdown-to-jsx": "^7.7.15",
    "monaco-editor": "0.54.0",
    "p-timeout": "^7.0.1",
    "react": "^19.2.0",
    "react-aria": "^3.44.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "patch:react-hook-form@npm%3A7.64.0#~/.yarn/patches/react-hook-form-npm-7.64.0-c738cfa2e9.patch",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.13",
    "react-stately": "^3.42.0",
    "valibot": "^1.1.0",
    "zustand": "^5.0.8"
  }
}
