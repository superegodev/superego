{
  "name": "@superego/browser-app",
  "version": "2026.2.2",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.tsx"
    },
    "./vite.config.js": {
      "import": "./vite.config.ts"
    }
  },
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:host": "USE_HTTPS=true vite --host",
    "clear-vite-cache": "rm -rf node_modules/.vite node_modules/.vite-temp",
    "check-types": "tsc --noEmit",
    "test": "vitest --reporter=verbose",
    "check-translations": "node scripts/checkTranslations.ts",
    "translations:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --id-interpolation-pattern='[name].[ext]_[sha512:contenthash:base64:6]' --out-file src/translations/en.json",
    "translations:compile": "formatjs compile-folder --ast src/translations/ src/translations/compiled",
    "translations:verify": "formatjs verify --missing-keys --structural-equality --source-locale en 'src/translations/*.json'",
    "translations:extract-and-compile": "yarn translations:extract && yarn translations:compile"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.4",
    "@formatjs/cli": "^6.12.2",
    "@superego/app-sandbox": "workspace:*",
    "@superego/connectors": "workspace:*",
    "@superego/demo-data-repositories": "workspace:*",
    "@superego/executing-backend": "workspace:*",
    "@superego/monaco-typescript-compiler": "workspace:*",
    "@superego/openai-compat-inference-service": "workspace:*",
    "@superego/quickjs-javascript-sandbox": "workspace:*",
    "@superego/schema": "workspace:*",
    "@superego/themes": "workspace:*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/luxon": "^3.7.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-basic-ssl": "^2.1.4",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "babel-plugin-formatjs": "^11.2.3",
    "playwright": "^1.58.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-bundle-analyzer": "^1.3.5",
    "vitest": "^4.0.18",
    "zx": "8.8.5-lite"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.20.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-aria/optimize-locales-plugin": "^1.1.5",
    "@standard-schema/spec": "^1.1.0",
    "@superego/backend": "workspace:*",
    "@superego/global-types": "workspace:*",
    "@superego/javascript-sandbox-global-utils": "workspace:*",
    "@superego/schema": "workspace:*",
    "@superego/shared-utils": "workspace:*",
    "@tanstack/react-query": "^5.90.20",
    "@tiptap/extension-code-block-lowlight": "^3.19.0",
    "@tiptap/extension-list": "^3.19.0",
    "@tiptap/extension-subscript": "^3.19.0",
    "@tiptap/extension-superscript": "^3.19.0",
    "@tiptap/extension-text-align": "^3.19.0",
    "@tiptap/extension-text-style": "^3.19.0",
    "@tiptap/extension-typography": "^3.19.0",
    "@tiptap/extensions": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "@vitejs/plugin-react": "^5.1.3",
    "debounce": "^3.0.0",
    "echarts": "^6.0.0",
    "es-toolkit": "^1.44.0",
    "frimousse": "^0.3.0",
    "lowlight": "^3.3.0",
    "luxon": "^3.7.2",
    "markdown-to-jsx": "^9.6.1",
    "monaco-editor": "0.55.1",
    "p-map": "^7.0.4",
    "p-timeout": "^7.0.1",
    "react": "^19.2.4",
    "react-aria": "^3.46.0",
    "react-aria-components": "^1.15.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-icons": "^5.5.0",
    "react-intl": "^8.1.3",
    "react-stately": "^3.44.0",
    "valibot": "^1.2.0",
    "zustand": "^5.0.11"
  }
}
