{
  "overrides": [
    {
      "files": "*.yml",
      "options": { "printWidth": 120 }
    },
    {
      "files": "*.md",
      "options": { "proseWrap": "always" }
    },
    {
      "files": ["*.sql"],
      "options": { "parser": "sqlite" }
    },
    {
      "files": "*.astro",
      "options": { "parser": "astro" }
    }
  ],
  "plugins": ["prettier-plugin-sql-cst", "prettier-plugin-astro"]
}
